{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Описание вашего расширения",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js", "scripts/renderMenuYT.js",  "scripts/test1.js", "scripts/changeSpeed.js", "scripts/speedSetting.js"],
      "css": ["styles/youtubeElements.css"]
    }
  ]
  
}
