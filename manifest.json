{
  "manifest_version": 3,
  "name": "YouTube Custom Playback Speed",
  "version": "1.0",
  "description": "Set your values for video playback speed. Change the playback speed in 1 click.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "permissions": [
    "tabs"
  ],

  "host_permissions": [
    "https://*.youtube.com/*",
    "https://*.buymeacoffee.com/khaliavkin"

  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["scripts/renderMenuYT.js", "scripts/databaseSetting.js", "scripts/changeSpeed.js", "scripts/speedSetting.js"],
      "css": ["styles/youtubeElements.css"]
    }
  ]
  
}


